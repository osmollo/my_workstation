{% for item in environment_variables %}
{% if not (item.name | regex_search("^FZF")) %}
{% if item.type | default ('str') == 'str' %}
export {{ item.name }}='{{ item.value }}'
{% else%}
export {{ item.name }}={{ item.value }}
{% endif %}
{% endif %}
{% endfor %}

if type rg &> /dev/null; then
{% for item in environment_variables %}
{% if (item.name | regex_search("^FZF")) %}
  export {{ item.name }}="{{ item.value }}"
{% endif %}
{% endfor %}
fi

[connection]
id=ProtonVPN
uuid={{ protonvpn.vpn_uuid }}
type=vpn
permissions=

[vpn]
auth=SHA512
ca={{ ansible_user_dir }}/.cert/nm-openvpn/connect-ca.pem
cipher=AES-256-CBC
comp-lzo=no-by-default
connection-type=password
dev=tun
mssfix=1450
password-flags=1
remote={{ remotes }}
remote-cert-tls=server
remote-random=yes
reneg-seconds=0
ta={{ ansible_user_dir }}/.cert/nm-openvpn/connect-tls-auth.pem
ta-dir=1
tunnel-mtu=1500
username={{ protonvpn.user }}
service-type=org.freedesktop.NetworkManager.openvpn

[ipv4]
dns-search=
method=auto

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto
